{
    "name": "danepowell/dp6502",
    "description": "Emulator for 6502 MPU and associated hardware",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Danepowell\\dp6502\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Danepowell\\dp6502\\Tests\\": "tests/src/"
        }
    },
    "authors": [
        {
            "name": "Dane Powell",
            "email": "git@danepowell.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "acquia/coding-standards": "^0.7.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "phpcodesniffer-search-depth": "4"
    }
}
